{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "web socket server app",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../build/moboware/debug/web_socket_server_app",
      "cwd": "${workspaceFolder}/../build/moboware/debug/",
      "miDebuggerPath": "/usr/bin/gdb",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "common_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../build/moboware/debug/common_test",
      "args": [
        "--gtest_filter=*"
      ],
      "cwd": "${workspaceFolder}/../build/moboware/debug/",
      "miDebuggerPath": "/usr/bin/gdb",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "modules_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../build/moboware/debug/modules_test",
      "args": [
        "--gtest_filter=*.MatchOrderFullTradeBidAndAskSideTest"
      ],
      "cwd": "${workspaceFolder}/../build/moboware/debug/",
      "miDebuggerPath": "/usr/bin/gdb",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "varys_test",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/../build/moboware/debug/varys_test",
      "args": [
        "--gtest_filter=*.test2"
      ],
      "cwd": "${workspaceFolder}/../build/moboware/debug/",
      "miDebuggerPath": "/usr/bin/gdb",
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    }
  ]
}